
<% @posts.each do |post| %>

  <%= post.artist_id %>

  <%= post.title %>

  <%= image_tag post.image.url %>

  <%= post.introduction %>

  <%= post.schedule %>

  <%= post.watching_method %>

  <%= post.detailed_url %>

  <%= post.admission %>

  <%= link_to post_path(post) do %>
    andmore<br>
  <% end %>

<% end %>